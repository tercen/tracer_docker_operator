{
  "name": "Tracer",
  "description": "Quantify TCR reads with TraCeR",
  "tags": ["genomics", "transcriptomics", "rna-seq", "fastq", "t-cell receptor"],
  "authors": ["tercen"],
  "urls": ["https://github.com/tercen/tracer_docker_operator"],
  "container": "tercen/TraCeR_docker_operator:latest",
  "properties": [
    {
      "kind": "EnumeratedProperty",
      "name": "save_output_to_folder",
      "defaultValue": "yes",
      "values": [
        "yes",
        "no"
      ]
    },
    {
      "kind": "StringProperty",
      "name": "output_folder_prefix",
      "defaultValue": "sequence_"
    }
  ]
}
